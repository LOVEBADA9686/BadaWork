	Connection conn = DriverManager.getConnection("jdbc:oracle:thin:@localhost:1521:orclKbd", "scott", "tiger");
	Statement stmt = conn.createStatement();
	ResultSet rsRs = stmt.executeQuery("SELECT * FROM EMP");
	while (rsRs.next()){
		out.println(rsRs.getString(1) + "<br>");
