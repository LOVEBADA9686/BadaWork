<script type="text/javascript">
	
	// 체크박스 체크 확인하기
	function checkOnly(chk){
		// alert(">>> : " + chk);
		var chkObj =
		document.getElementsByName("bnumCheck");
		console.log("chkObj >>> : " + chkObj);
		for (var i=0; i < chkObj.length; i++){
			if(chkObj[i] !=chk){
				chkObj[i].checked = false;
			}
		}
	}

